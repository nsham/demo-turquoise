<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Contact Information Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <basic
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <leftlinks
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                eaem-nested=""
                                fieldLabel="Contact Information">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    name="./contactinfo">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <image
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="Image"
                                                    name="./image"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam/USGBoral"/>
                                                <pagepath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="Page Path"
                                                    name="./pagepath"
                                                    required="{Boolean}false"
                                                    rootPath="/content/usgboral"/>
                                                <country
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    fieldDescription="Used for filtering"
                                                    fieldLabel="Country"
                                                    name="./country">
                                                    <datasource
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="usgbv3/components/structure/common/datasources/country"/>
                                                </country>
                                                <title/>
                                                <richtext/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </leftlinks>
                        </items>
                    </column>
                </items>
            </basic>
            <desktop
                jcr:primaryType="nt:unstructured"
                jcr:title="Dekstop"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <align
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Filter alignment"
                                name="./aligndesktop">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="/apps/usgb/components/structure/common/datasources/alignment"/>
                            </align>
                            <columns/>
                        </items>
                    </column>
                </items>
            </desktop>
            <tablet
                jcr:primaryType="nt:unstructured"
                jcr:title="Tablet"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <align_tab
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Filter alignment"
                                name="./aligntablet">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="/apps/usgb/components/structure/common/datasources/alignment"/>
                            </align_tab>
                            <columns_tab/>
                        </items>
                    </column>
                </items>
            </tablet>
            <mobile
                jcr:primaryType="nt:unstructured"
                jcr:title="Mobile"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <align_mobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Filter alignment"
                                name="./alignmobile">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="/apps/usgb/components/structure/common/datasources/alignment"/>
                            </align_mobile>
                            <columns_mobile/>
                        </items>
                    </column>
                </items>
            </mobile>
        </items>
    </content>
</jcr:root>
